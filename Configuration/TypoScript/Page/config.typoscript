# performance optimization
# Note: compressJs, compressCss, concatenateCss, concatenateJs were removed in TYPO3 12+
# Use external build tools (webpack, vite) or AssetCollector for asset handling
config {
    # sourceopt
    sourceopt {
        enabled = 1
        formatHtml = 2
    }
    # allow indexing
    index_enable = 1
}